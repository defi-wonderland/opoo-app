{
  "name": "opoo-app",
  "description": "Oppo UI App",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "license": "AGPL-3.0-only",
  "author": "Wonderland",
  "contributors": [
    "0xArdy (https://github.com/0xArdy)",
    "turtlemoji (https://github.com/turtlemoji)"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "yarn run lint -- --fix",
    "prettier": "prettier src --check",
    "prettier:fix": "yarn run prettier -- --write",
    "format": "yarn run prettier:fix && yarn run lint:fix",
    "format:check": "yarn run prettier && yarn run lint",
    "prepare": "husky install && wonderland-crypto-husky-checks install",
    "test": "vitest run --dir tests/",
    "test:watch": "vitest --dir tests/"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,ts,tsx,css,scss,md,html}": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "ethers": "6.6.5",
    "opoo-sdk": "0.0.0-bb9c74cd",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.14.2",
    "react-transition-group": "4.4.5",
    "styled-components": "6.0.5"
  },
  "devDependencies": {
    "@defi-wonderland/crypto-husky-checks": "1.3.0",
    "@types/react": "18.2.16",
    "@types/react-dom": "18.2.7",
    "@types/react-transition-group": "4.4.6",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "@vitejs/plugin-react-swc": "3.3.2",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-react": "7.33.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "husky": "8.0.3",
    "prettier": "3.0.0",
    "typescript": "5.1.6",
    "vite": "4.4.7",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.33.0"
  }
}
